#SET SERVEROUTPUT ON;

DECLARE
  num1 INTEGER;
  num2 INTEGER;
  sum1 INTEGER;
  diff INTEGER;
  prod INTEGER;
  div  Float;
  
  
BEGIN
    
    num1 := &Enter_num1;
    num2 := &Enter_num2;
    sum1 := num1 + num2;
    diff := num1 - num2;
    prod := num1 * num2;
    div  := num1 / num2;

    DBMS_OUTPUT.PUT_LINE(sum1);
    DBMS_OUTPUT.PUT_LINE(diff);    
    DBMS_OUTPUT.PUT_LINE(prod);
    DBMS_OUTPUT.PUT_LINE(div);
END;
/

SET SERVEROUTPUT ON;

DECLARE
  num1 INTEGER;
  num2 INTEGER;
  sum1 INTEGER;
  diff INTEGER;
  prod INTEGER;
  div  Float;
  
  
